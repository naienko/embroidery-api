{
  "name": "shoppinglist",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "number"
    },
    "userId": {
      "type": "number",
      "required": true
    },
    "identifierId": {
      "type": "number",
      "required": true
    },
    "notes": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "stitchers": {
      "type": "hasOne",
      "model": "stitcher",
      "foreignKey": "id",
      "options": {
        "nestRemoting": true
      }
    },
    "fibers": {
      "type": "hasMany",
      "model": "identifier",
      "foreignKey": "id",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superuser",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
